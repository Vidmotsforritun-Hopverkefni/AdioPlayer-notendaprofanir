<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="400.0" maxWidth="250.0" prefHeight="400.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hi.vidmot.audioplayer.controller.PlayerController">
   <children>
       <fx:include fx:id="menuStyring" source="menu_lagalisti-view.fxml" />
       <Label fx:id="img_lagalisti" alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="100.0">
           <VBox.margin>
               <Insets />
           </VBox.margin>
       </Label>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="fxLogin" mnemonicParsing="false" onAction="#onLogin" text="Áskrifandi" />
         </children>
      </HBox>
      <ListView fx:id="fxListView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" onMouseClicked="#onValidLaglista" />
<!--      <ListView prefHeight="200.0" prefWidth="200.0" />-->
      <Button fx:id="fxSelect" disable="true" mnemonicParsing="false" onAction="#onSelect" text="Slelect" />
   </children>
</VBox>
